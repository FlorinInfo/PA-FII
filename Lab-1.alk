//Write an algorithm that generates the primes with Eratosthenes sieve
getFirstPrime(n) {
    for(i = 0;i <= n;i++ ) {
        v[i] = 0;
    }
    v[0] =  1;
    v[1] = 1;
    for(i = 2; i*i <= n;i++){
        if(v[i] == 0) {
            for(j = 2;j <= n/i; j++) {
                v[i*j] = 1;
            }
        }
    }
    for(i = 0;i <= n;i++ ) {
        if(v[i]==0) print(i);
    }
}

getFirstPrime(n);



